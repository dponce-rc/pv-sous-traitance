{
    "_version": "1.12.0",
    "sap.app": {
        "id": "PP.ZManage_PV",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "sourceTemplate": {
            "id": "servicecatalog.connectivityComponentForManifest",
            "version": "0.0.0",
            "toolsId": "a40bac3a-0b3c-4071-a17a-2d86f8555204"
        },
        "dataSources": {
            "ZPRJ_PV_SUBCONTRACTOR_SRV": {
                "uri": "/sap/opu/odata/sap/ZPRJ_PV_SUBCONTRACTOR_SRV/",
                "type": "OData",
                "settings": {
                    "localUri": "localService/metadata.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "PP.ZManage_PV.view.View1",
            "type": "XML",
            "async": true,
            "id": "View1"
        },
        "dependencies": {
            "minUI5Version": "1.65.6",
            "libs": {
                "sap.ui.layout": {},
                "sap.ui.core": {},
                "sap.m": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "PP.ZManage_PV.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.odata.v2.ODataModel",
                "settings": {
                    "defaultOperationMode": "Server",
                    "defaultBindingMode": "OneWay",
                    "defaultCountMode": "Request"
                },
                "dataSource": "ZPRJ_PV_SUBCONTRACTOR_SRV",
                "preload": true
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "PP.ZManage_PV.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false,
                "targetClass": "sap.m.routing.Router",
                "bypassed": {
                    "target": "notFound"
                }
            },
            "routes": [
                {
                    "name": "RouteView1",
                    "pattern": "",
                    "targetAggregation": "Pages",
                    "greedy": false,
                    "target": [
                        "RouteView1"
                    ]
                },
                {
                    "name": "DisplayPV",
                    "pattern": "DisplayPV/{Ebeln}/{Ebelp}",
                    "targetAggregation": "Pages",
                    "greedy": false,
                    "target": [
                        "DisplayPV"
                    ]
                },
                {
                    "name": "Update",
                    "pattern": "Update/{Qmnum}",
                    "targetAggregation": "Pages",
                    "greedy": false,
                    "target": [
                        "Update"
                    ]
                },
                {
                    "name": "CreatePV",
                    "pattern": "CreatePV/{Ebeln}/{Ebelp}/{Txz01}",
                    "targetAggregation": "Pages",
                    "greedy": false,
                    "target": [
                        "CreatePV"
                    ]
                }
            ],
            "targets": {
                "DisplayPV": {
                    "viewType": "XML",
                    "viewName": "DisplayPV",
                    "clearControlAggregation": false
                },
                "RouteView1": {
                    "viewType": "XML",
                    "viewId": "RouteView1",
                    "viewName": "View1"
                },
                "Update": {
                    "viewType": "XML",
                    "viewName": "Update"
                },
                "CreatePV": {
                    "viewType": "XML",
                    "viewName": "CreatePV"
                },
                "BatchSh": {
                    "viewType": "XML",
                    "viewName": "BatchSh"
                }
            }
        }
    },
    "sap.platform.abap": {
        "uri": "/sap/bc/ui5_ui5/sap/zmanage_pv/webapp",
        "_version": "1.1.0"
    }
}